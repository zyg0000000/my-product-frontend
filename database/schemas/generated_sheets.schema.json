{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "_id",
    "createdAt",
    "createdBy",
    "fileName",
    "projectId",
    "sheetToken",
    "sheetUrl"
  ],
  "properties": {
    "_id": {
      "$ref": "#/$defs/ObjectId"
    },
    "createdAt": {
      "$ref": "#/$defs/Date"
    },
    "createdBy": {
      "type": "string"
    },
    "fileName": {
      "type": "string"
    },
    "projectId": {
      "type": "string"
    },
    "sheetToken": {
      "type": "string"
    },
    "sheetUrl": {
      "type": "string"
    }
  },
  "$defs": {
    "ObjectId": {
      "type": "object",
      "properties": {
        "$oid": {
          "type": "string",
          "pattern": "^[0-9a-fA-F]{24}$"
        }
      },
      "required": [
        "$oid"
      ],
      "additionalProperties": false
    },
    "Date": {
      "type": "object",
      "properties": {
        "$date": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": [
        "$date"
      ],
      "additionalProperties": false
    }
  }
}